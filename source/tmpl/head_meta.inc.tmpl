<meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1" />
<meta charset="utf-8" />

<meta property="og:url" content="{{= full_url }}" />
<meta property="og:title" content="{{= og_title }}" />
<meta property="og:type" content="article" />
<meta property="og:description" content="{{= og_description }}" />
<meta property="og:site_name" content="BBC News" />
<meta property="og:image" content="{{= pathStatic }}/{{= vocab_dir }}{{= share_img_path }}" />

<meta name="twitter:card" content="summary_large_image">
<meta name="twitter:site" content="@BBCNews">
<meta name="twitter:creator" content="@BBCNews">
<meta name="twitter:title" content="{{= twitter_title}}">
<meta name="twitter:description" content="{{= twitter_description }}">
<meta name="twitter:image" content="{{= pathStatic }}/{{= vocab_dir }}{{= share_img_path }}">
